{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [ "src" ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "../../../node_modules/docdash",
    "encoding": "utf8",
    "destination": "build/docs",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "default": {
      "includeDate": false
    },
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "collapse": true,
    "search": true
  }
}

{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "docs/site/Home.md",
      "packages/@lse/core/src",
      "packages/@lse/react/src",
      "packages/@lse/loader/src"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "tutorials": "docs/site/tutorials",
    "encoding": "utf8",
    "destination": "build/docs/site",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "default": {
      "includeDate": false
    },
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "collapse": true,
    "search": true,
    "sectionOrder": [
      "Modules",
      "Tutorials",
      "Classes",
      "Events",
      "Externals",
      "Mixins",
      "Interfaces"
    ]
  }
}
